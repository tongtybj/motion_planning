<?xml version="1.0" encoding="utf-8"?>
<launch>
  <arg name="model" default="Hydrus_hex" /> 
  <arg name="debug" default="false" />
  <arg unless="$(arg debug)" name="launch_prefix" value="" />

<node name="posture_optimization_node" pkg="hydrus_gap_passing" type="posture_optimization_node" respawn="false" launch-prefix="$(arg launch_prefix)" output="screen">
       <!-- for transform controller -->
    <rosparam file="$(find hydrus_transform_control)/config/hydrusx/$(arg model).yaml" command="load" />
   </node>
</launch>
